section.wrapper{

	.loadContent{
		padding 1.5em 10px 15px
		h1{
			text-align center
			text-transform uppercase
			font-weight 600
		}
		h2{
			-webkit-box-shadow: 0px -42px 3px -34px headerColor
			-moz-box-shadow: 0px -42px 3px -34px headerColor
			box-shadow: 0px -42px 3px -34px headerColor
			padding-top 20px
		}
		a{
			color secondaryBlue
			&:hover{
				color defaultBLue
			}
		}
		ul{
			list-style-type circle
		}
		hr{
			border-color headerColor
		}
	}

}