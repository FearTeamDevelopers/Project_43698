.homepage section.wrapper{
	padding-top 40px
	padding-bottom 40px
	@import"button-more"
	.container{
		header {
			h3 {
				width 25%
				min-width 150px
				margin 0 auto;
				margin-bottom 20px;
				text-align center;
				padding-bottom 10px
				text-transform uppercase
				border-bottom 2px solid defaultGreen
				color black
				font-weight 700
			}
		}
		@import "bxslider"
		.container-box{
			background transparent
			position relative
			height 100%

			@import "novinky"
			@import "akce"
			@import "partneri"
		}
		.partneri{
			float left
			width 100%
		}

	}
}
