.moreWrap{
	height 40px
	line-height 40px
	width:140px;
	margin 0 auto
	margin-top 1.875rem
	.more{
		position relative
		text-align center
		height 40px;
		line-height 40px;
		background defaultGreen
		span{
			height 40px
			line-height 40px
			width 100%
			color white
		}
		&.blue:hover{
			background white
			span{
				color black
			}
		}
		&:hover{
			background headerColor
			span{
				color white
			}
		}
	}
}