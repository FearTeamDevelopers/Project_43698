.navWrapper{
	position absolute
	bottom 0%
	right 0%
	padding 0
	margin 0
	nav{
		float right
		bottom 10px
		ul{
			position relative
			float right
			margin-left 1rem
			list-style-type: none;
			li{
				display block
				padding-left 20px
				padding-right 20px
				border 0
				&:last-child{
					border 0px
					padding-right 0
				}
			}

			a{
				color white
				font-size 1.25rem
				&:hover{
					color defaultGreen
				}
			}

			li{
				display inline
				border-right 1px solid white
				padding-right 15px
				padding-left 15px
			}
		}
	}
}